.table-results-main
 .table-results
  .table-results-header
   .table-results-tr
    .table-results-th Trade
    .table-results-th Type
    .table-results-th SIgnal
    .table-results-th Date/Time
    .table-results-th Price
    .table-results-th Contracts
    .table-results-th Profit
    .table-results-th Balance
   hr.line-table
   
   mixin table-results(num, name, sell, date, price, contact, profit, balance, num2, name2, sell2, date2, price2, contact2, profit2, balance2)
    .table-results-tr
     .table-results-td= num
     .table-results-td= name
     .table-results-td.sell= sell 
     .table-results-td= date 
     .table-results-td= price 
     .table-results-td= contact 
     .table-results-td= profit 
     .table-results-td= balance

   mixin table-results2(num2, name2, sell2, date2, price2, contact2, profit2, balance2)
    .table-results-tr
     .table-results-td= num2 
     .table-results-td= name2 
     .table-results-td.buy= sell2
     .table-results-td= date2 
     .table-results-td= price2 
     .table-results-td= contact2 
     .table-results-td= profit2 
     .table-results-td= balance2
    hr.line-table


div(class="specification" id="Specs")
 .specification-in
  h2 Specification
  .table-specification
   .table-specification-header
    .table-specification-td Block, Start
    .table-specification-td Block, End
    .table-specification-td Reward
    .table-specification-td MN
    .table-specification-td PoS
    .table-specification-td ~ROI
   
